{
    "name": "tylercd100/laravel-licenser",
    "description": "Laravel - Define Licenses and Limitations",
    "keywords": [
        "laravel",
        "tylercd100"
    ],
    "homepage": "https://github.com/tylercd100/laravel-licenser",
    "license": "MIT",  
    "authors": [
        {
            "name": "Tyler Arbon",
            "email": "tylercd100@gmail.com"
        }
    ],
    "autoload":{
        "psr-4":{
            "Tylercd100\\License\\":"src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tylercd100\\License\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "laravel/framework": "^8.0",
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0|^6.3",
        "orchestra/testbench": "^6.0"
    },
    "suggest": {
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tylercd100\\License\\Providers\\LicenseServiceProvider"
            ]
        }
    }
}
